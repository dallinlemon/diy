{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Start",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "outputCapture": "std",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug Test-dev",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeVersion": "default",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "outputCapture": "std",
      "console": "integratedTerminal"
    }
  ]
}